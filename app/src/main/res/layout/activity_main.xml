    <Button
        android:id="@+id/navigasi3"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/navigasi4"
        android:layout_alignStart="@+id/navigasi4"
        android:layout_below="@+id/navigasi2"
        android:layout_marginTop="24dp"
        android:text="Navigasi 3"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="162dp" />

    <Button
        android:id="@+id/navigasi4"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/navigasi3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:text="Navigasi 4"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="298dp" />


</RelativeLayout>
